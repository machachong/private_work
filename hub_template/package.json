{
	"name": "vite-public_template",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc -b && vite build",
		"build:dev": "vue-tsc -b && vite build --mode dev",
		"preview": "vite preview",
		"prepare": "husky install",
		"prettier": "prettier  --write \"src/**/*.{ts,js,vue}\"",
		"lint": "eslint \"src/**/*.{ts,js,vue}\" \"src/main.ts\"",
		"lint:fix": "npx eslint --fix --ext .js,.ts,.vue,.jsx  src",
		"commit": "git-cz",
		"lint-staged": "lint-staged"
	},
	"dependencies": {
		"@wangeditor/editor": "^5.1.23",
		"@wangeditor/editor-for-vue": "^5.1.12",
		"@wangeditor/plugin-md": "^1.0.0",
		"axios": "^1.7.7",
		"echarts": "5.4.3",
		"element-plus": "2.8.5",
		"highlight.js": "^11.7.0",
		"html2canvas": "^1.4.1",
		"lodash": "^4.17.21",
		"markdown-it": "^13.0.2",
		"monaco-editor": "^0.51.0",
		"monaco-editor-webpack-plugin": "^7.1.0",
		"node-polyfill-webpack-plugin": "^3.0.0",
		"normalize.css": "^8.0.1",
		"nprogress": "^0.2.0",
		"path-browserify": "^1.0.1",
		"pinia": "2.1.1",
		"pinia-plugin-persistedstate": "3.2.1",
		"sass": "1.79.3",
		"sortablejs": "^1.15.3",
		"terser": "^5.36.0",
		"tributejs": "5.1.3",
		"vconsole": "^3.15.1",
		"vditor": "^3.10.8",
		"vue": "^3.5.10",
		"vue-diff": "^1.2.4",
		"vue-draggable-plus": "^0.6.0",
		"vue-router": "^4.4.5",
		"vue-tribute": "^2.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "12.1.4",
		"@commitlint/config-conventional": "12.1.4",
		"@types/node": "^22.7.5",
		"@typescript-eslint/eslint-plugin": "^5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"@vitejs/plugin-vue": "^4.0.0",
		"code-inspector-plugin": "^0.20.3",
		"commitizen": "^4.3.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.8.0",
		"globals": "^13.19.0",
		"husky": "^7.0.1",
		"lint-staged": "14.0.1",
		"prettier": "3.2.4",
		"rollup-plugin-visualizer": "^5.14.0",
		"typescript": "^4.9.3",
		"unplugin-auto-import": "^0.18.3",
		"unplugin-vue-components": "^0.27.4",
		"vite": "^4.4.9",
		"vite-plugin-svg-icons": "^2.0.1",
		"vue-tsc": "^1.8.27",
		"yarn": "^1.22.22"
	},
	"engines": {
		"node": "16.18.1",
		"npm": "8.19.2"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
